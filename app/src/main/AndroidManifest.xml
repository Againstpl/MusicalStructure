            android:parentActivityName="pl.against.musicalstructure.MainActivity"></activity>
        <activity
            android:name=".Album2Activity"
            android:parentActivityName="pl.against.musicalstructure.MainActivity"></activity>
        <activity
            android:name=".Album3Activity"
            android:parentActivityName="pl.against.musicalstructure.MainActivity"></activity>
        <activity
            android:name=".Album4Activity"
            android:parentActivityName="pl.against.musicalstructure.MainActivity"></activity>
        <activity
            android:name=".Album5Activity"
            android:parentActivityName="pl.against.musicalstructure.MainActivity"></activity>
        <activity
            android:name=".Album6Activity"
            android:parentActivityName="pl.against.musicalstructure.MainActivity"></activity>


    </application>

</manifest>


